@{
    ViewData["Title"] = "Home Page";
}
<header>
<div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-indicators">
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
    </div>
    <div class="carousel-inner">
            <div class="carousel-item active" style="background-image: url('https://w0.peakpx.com/wallpaper/654/173/HD-wallpaper-playboi-carti-is-closing-eyes-and-touching-hair-with-fingers-in-white-background-music.jpg')">
            <div class="carousel-caption">
                <h5>Whole Lotta Red</h5>
                <p>Playboi Carti Releases Finally Releases WLR</p>
            </div>
        </div>
            <div class="carousel-item" style="background-image: url('https://w0.peakpx.com/wallpaper/339/517/HD-wallpaper-drake-drake-music-singer.jpg')">
            <div class="carousel-caption">
                <h5>Her Loss</h5>
                <p>Drake Releases Colaboration with 21 Savage</p>
            </div>
        </div>
            <div class="carousel-item" style="background-image: url('https://w0.peakpx.com/wallpaper/824/697/HD-wallpaper-asap-rocky-minimalsim-american-rapper-music-stars-creative-fan-art-rakim-mayers-american-celebrity-red-backgrounds.jpg')">
            <div class="carousel-caption">
                <h5>Flacko Minimalist</h5>
                <p>ASAP Rocky teases new album in latest news</p>
            </div>
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>
</header>

<!-- Page Content -->
<section class="py-5">
    <div class="container">
        <h1 class="fw-light">Meet the Team</h1>
        <p class="lead">
            We are a team of passionate music enthusiasts dedicated to providing you with up-to-date news, album reviews, artist interviews, and concert coverage. Our goal is to help you discover new music and to keep you informed about what's happening in the music industry. We provide honest and unbiased reviews of new albums and singles, so you can decide whether they're worth your time and money. Our interviews give you an inside look at the creative process behind the music, and our concert coverage lets you know the best spots to catch a show. So what are you waiting for? Start exploring today!
        </p>
    </div>
</section>